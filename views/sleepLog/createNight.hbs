<h1>Log Night</h1>

<form id="create-night" method="POST" action="./createNight">
  <label for="creating-night">
    <p>Date<input type="text" name="date" placeholder="mm/dd/yy"></p>
    <p>Hours Slept<input type="text" name="hoursSlept"></p>
    <p>Bedtime<input type="text" name="bedtime"></p>
    <p>Room Temperature<input type="text" name="temp"></p>
    <input type="submit" value="submit">
  </label>
</form>